{
    "manifest_version": 3,
    "name": "LinkedIn Scraper",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage", "identity"],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["https://script.google.com/macros/s/"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  